@model DoItYourself.Web.ViewModels.Home.HomeIndexViewModel

<div class="row">
    <div style="width: 100%">
        <img src="http://i67.tinypic.com/160r9yr.png" style="width: 100%" />
    </div>
</div>

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            The Newest Projects
        </h1>
    </div>
    @foreach (var project in Model.Projects)
    {
        <div class="col-md-4">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h4><i class="fa fa-fw fa-bookmark-o"></i>
                     @project.Title.Substring(0, 20)...
                    </h4>
                </div>
                <div class="panel-body">
                    <p>
                        @project.Content.Substring(0, 85)...
                    </p>
                    @Html.ActionLink("Lear More", "Details", "Projects", new { id = project.Id }, null)
                </div>
            </div>
        </div>
    }
</div>

